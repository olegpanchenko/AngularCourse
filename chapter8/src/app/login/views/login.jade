.container
  .well.col-md-6.col-md-offset-3
    form(ng-submit='login()')
      .form-group
        input#user-name.form-control(type='text', ng-model='userName', placeholder='User Name')
      .form-group
        input#password.form-control(type='password', ng-model='password', placeholder='Password')
      button.btn.btn-primary(type='submit') Submit
